{
  "compilerOptions": {
     "lib": [
        "es5",
        "es6"
     ],
     "target": "es5",
     "esModuleInterop": true,
     "module": "commonjs",
     "moduleResolution": "node",
     "rootDir": "src",
     "outDir": "./build",
     "forceConsistentCasingInFileNames": true,
     "strict": true,
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "sourceMap": true
  }
}
